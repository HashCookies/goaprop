<div class="col-md-12 edit-intro">
	<h3 class="title">Create A New Property</h3>
</div>


<form role="form" action="/create" method="post" id="property" enctype="multipart/form-data">
	
	<div class="col-md-8">
		<div class="form-group">
				<label>Property Name:</label><br />
				<input type="text" name="property[title]" id="title" class="form-control" />	
		</div>
		
		
		<div class="form-group">
				<label>Description:</label><br />
				<input type="text" name="property[desc]" id="title" class="form-control" />	
		</div>
		
		<div class="file-input-wrap">
			<input type="file" name="images[]" title="+ Gallery Images"  class="file-picker file-inputs new-file" data-filename-placement="inside"  multiple />
		</div>
		

		<div class="form-group">
				<input type="Submit" class="btn btn-primary btn-large submit-btn" value="Create New Property">
			</div>
	</div>

	
</form>
